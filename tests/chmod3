!touch input
chmod 600 input
ls -l input
#-rw------- +\? +\d+ +\d+ +0 +[a-zA-Z]+ +\d+ +\d+:\d\d input
put -P input output
ls -l output
#-rw------- +\? +\d+ +\d+ +0 +[a-zA-Z]+ +\d+ +\d+:\d\d output
chmod 600 nosuchfile
#.*file does not exist.*
chmod --raw 700 ""
ls -ld .
#drwx------ +\? +\d+ +\d+ +\d+ +[a-zA-Z]+ +\d+ +\d+:\d\d .*
